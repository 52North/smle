<?xml version="1.0" encoding="UTF-8"?>
<p:profile xmlns:p="http://xmlProfile.de" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlProfile.de Profile2.xsd" class="sml:PhysicalSystem">


	<p:elementString XPath="gml:description" ID="description">
		<p:input>
			<p:textField />
		</p:input>
	</p:elementString>

	<p:elementGroup groupID="KeywordList" XPath="sml:keywords/sml:KeywordList">
		<p:elements>
			<p:elementString ID="keyword_KeywordList" XPath="sml:keyword">
				<p:restrictions>
					<p:use>optional</p:use>
				</p:restrictions>
				<p:input>
					<p:textField />
				</p:input>
			</p:elementString>
		</p:elements>
	</p:elementGroup>

	<p:elementGroup groupID="Identification" XPath="sml:Identification/sml:IdentifierList">
		<p:elements>
			<p:elementGroupRef groupRef="uniqueID"
				XPath="sml:Identification/sml:IdentifierList/sml:Identifier/sml:Term" />
			<p:elementGroupRef groupRef="shortName"
				XPath="sml:Identification/sml:IdentifierList/sml:Identifier/sml:Term" />
			<p:elementGroupRef groupRef="longName"
				XPath="sml:Identification/sml:IdentifierList/sml:Identifier/sml:Term" />
			<p:elementGroupRef groupRef="deviceID"
				XPath="sml:Identification/sml:IdentifierList/sml:Identifier/sml:Term" />
			<p:elementGroupRef groupRef="serialNumber"
				XPath="sml:Identification/sml:IdentifierList/sml:Identifier/sml:Term" />
			<p:elementGroupRef groupRef="WMO"
				XPath="sml:Identification/sml:IdentifierList/sml:Identifier/sml:Term" />
		</p:elements>
	</p:elementGroup>

	<p:elementGroup groupID="shortName"
		XPath="sml:Identification/sml:IdentifierList/sml:Identifier/sml:Term">
		<p:elements>
			<p:elementString XPath="sml:label" ID="label_shortName">
				<p:restrictions>
					<p:fixContent>
						<p:value>Short name</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementString>
			<p:elementAnyURI XPath="@definition" ID="definition_shortName">
				<p:restrictions>
					<p:fixContent>
						<p:value>http://www.nexosproject.eu/dictionary/definitions.html#shortName</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementAnyURI>
			<p:elementString XPath="swe:value" ID="value_shortName">
				<p:label>Short name (swe:value)</p:label>
				<p:input>
					<p:textField />
				</p:input>
			</p:elementString>
		</p:elements>
	</p:elementGroup>
	<p:elementGroup groupID="longName"
		XPath="sml:Identification/sml:IdentifierList/sml:Identifier/sml:Term">
		<p:elements>
			<p:elementString XPath="sml:label" ID="label_longName">
				<p:restrictions>
					<p:fixContent>
						<p:value>Long name</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementString>
			<p:elementAnyURI XPath="@definition" ID="definition_longName">
				<p:restrictions>
					<p:fixContent>
						<p:value>http://www.nexosproject.eu/dictionary/definitions.html#longName</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementAnyURI>
			<p:elementString XPath="swe:value" ID="value_longName">
				<p:label>Long name (swe:value)</p:label>
				<p:input>
					<p:textField />
				</p:input>
			</p:elementString>
		</p:elements>
	</p:elementGroup>
	<p:elementGroup groupID="uniqueID"
		XPath="sml:Identification/sml:IdentifierList/sml:Identifier/sml:Term">
		<p:elements>
			<p:elementString XPath="sml:label" ID="label_uniqueID">
				<p:restrictions>
					<p:fixContent>
						<p:value>UUID</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementString>
			<p:elementAnyURI XPath="@definition" ID="definition_uniqueID">
				<p:restrictions>
					<p:fixContent>
						<p:value>http://www.nexosproject.eu/dictionary/definitions.html#UUID</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementAnyURI>
			<p:elementString XPath="swe:value" ID="value_uniqueID">
				<p:label>Unique ID (swe:value)</p:label>
				<p:input>
					<p:textField />
				</p:input>
			</p:elementString>
		</p:elements>
	</p:elementGroup>
	<p:elementGroup groupID="deviceID"
		XPath="sml:Identification/sml:IdentifierList/sml:Identifier/sml:Term">
		<p:elements>
			<p:elementString XPath="sml:label" ID="label_deviceID">
				<p:restrictions>
					<p:fixContent>
						<p:value>Device ID</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementString>
			<p:elementAnyURI XPath="@definition" ID="definition_deviceID">
				<p:restrictions>
					<p:fixContent>
						<p:value>http://www.nexosproject.eu/dictionary/definitions.html#deviceID</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementAnyURI>
			<p:elementString XPath="swe:value" ID="value_deviceID">
				<p:label>Device ID (swe:value)</p:label>
				<p:input>
					<p:textField />
				</p:input>
			</p:elementString>
		</p:elements>
	</p:elementGroup>
	<p:elementGroup groupID="serialNumber"
		XPath="sml:Identification/sml:IdentifierList/sml:Identifier/sml:Term">
		<p:elements>
			<p:elementString XPath="sml:label" ID="label_serialNumber">
				<p:restrictions>
					<p:fixContent>
						<p:value>Serial number</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementString>
			<p:elementAnyURI XPath="@definition" ID="definition_serialNumber">
				<p:restrictions>
					<p:fixContent>
						<p:value>http://www.nexosproject.eu/dictionary/definitions.html#serialNumber</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementAnyURI>
			<p:elementString XPath="swe:value" ID="value_serialNumber">
				<p:label>Serial number (swe:value)</p:label>
				<p:input>
					<p:textField />
				</p:input>
			</p:elementString>
		</p:elements>
	</p:elementGroup>
	<p:elementGroup groupID="WMO"
		XPath="sml:Identification/sml:IdentifierList/sml:Identifier/sml:Term">
		<p:elements>
			<p:elementString XPath="sml:label" ID="label_WMO">
				<p:restrictions>
					<p:fixContent>
						<p:value>WMO</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementString>
			<p:elementAnyURI XPath="@definition" ID="definition_WMO">
				<p:restrictions>
					<p:fixContent>
						<p:value>http://www.nexosproject.eu/dictionary/definitions.html#WMO_ID</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementAnyURI>
			<p:elementString XPath="swe:value" ID="value_WMO">
				<p:label>WMO (swe:value)</p:label>
				<p:input>
					<p:textField />
				</p:input>
			</p:elementString>
		</p:elements>
	</p:elementGroup>
	<p:elementGroup groupID="TimePeriod" XPath="sml:validTime/gml:TimePeriod">
		<p:elements>
			<p:elementString ID="ID_TimePeriod" XPath="gml:id">
				<p:restrictions>
					<p:fixContent>
						<p:value>validityPeriod</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementString>
		</p:elements>
	</p:elementGroup>

	<p:elementGroup groupID="Contact"
		XPath="sml:contact/sml:ContactList/sml:contact/gmd:CI_ResponsibleParty">
		<p:elements>
			<p:elementString ID="organisationName_Contact" XPath="gmd:organisationName">
				<p:input>
					<p:textField />
				</p:input>
			</p:elementString>
		</p:elements>
	</p:elementGroup>

	<p:elementGroup groupID="Position" XPath="sml:position/swe:DataRecord">
		<p:elements>
			<p:elementGroup groupID="longitude_field" XPath="swe:field">
				<p:elements>
					<p:elementNCName ID="name_longitude_field" XPath="@name">
						<p:restrictions>
							<p:fixContent>
								<p:value>longitude</p:value>
							</p:fixContent>
						</p:restrictions>
					</p:elementNCName>
					<p:elementGroupRef groupRef="longitude_Position" XPath="swe:field/swe:Quantity" />
				</p:elements>
			</p:elementGroup>
			<p:elementGroup groupID="latitude_field" XPath="swe:field">
				<p:elements>
					<p:elementNCName ID="name_latitude_field" XPath="@name">
						<p:restrictions>
							<p:fixContent>
								<p:value>latitude</p:value>
							</p:fixContent>
						</p:restrictions>
					</p:elementNCName>
					<p:elementGroupRef groupRef="latitude_Position" XPath="swe:field/swe:Quantity" />
				</p:elements>
			</p:elementGroup>
			<p:elementGroup groupID="altitude_field" XPath="swe:field">
				<p:elements>
					<p:elementNCName ID="name_altitude_field" XPath="@name">
						<p:restrictions>
							<p:fixContent>
								<p:value>altitude</p:value>
							</p:fixContent>
						</p:restrictions>
					</p:elementNCName>
					<p:elementGroupRef groupRef="altitude_Position" XPath="swe:field/swe:Quantity" />
				</p:elements>
			</p:elementGroup>
		</p:elements>
	</p:elementGroup>

	<p:elementGroup groupID="longitude_Position" XPath="swe:field/swe:Quantity">
		<p:elements>
			<p:elementAnyURI ID="referenceFrame_longitude_Position" XPath="@referenceFrame">
				<p:restrictions>
					<p:fixContent>
						<p:value>urn:ogc:crs:EPSG:4326</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementAnyURI>
			<p:elementString ID="axisID_longitude_Position" XPath="@axisID">
				<p:restrictions>
					<p:fixContent>
						<p:value>Lon</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementString>
			<p:elementString ID="code_uom_longitude_Position" XPath="swe:uom/@code">
				<p:restrictions>
					<p:defaultContent>
						<p:value>deg</p:value>
					</p:defaultContent>
				</p:restrictions>
				<p:input>
					<p:textField />
				</p:input>
			</p:elementString>
			<p:elementDouble ID="value_longitude_Position" XPath="swe:value">
				<p:input>
					<p:textField />
				</p:input>
			</p:elementDouble>
		</p:elements>
	</p:elementGroup>

	<p:elementGroup groupID="latitude_Position" XPath="swe:field/swe:Quantity">
		<p:elements>
			<p:elementAnyURI ID="referenceFrame_latitude_Position" XPath="@referenceFrame">
				<p:restrictions>
					<p:fixContent>
						<p:value>urn:ogc:crs:EPSG:4326</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementAnyURI>
			<p:elementString ID="axisID_latitude_Position" XPath="@axisID">
				<p:restrictions>
					<p:fixContent>
						<p:value>Lat</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementString>
			<p:elementGroup groupID="uom_latitude_Position" XPath="swe:uom">
				<p:elements>
					<p:elementString ID="code_uom_latitude_Position" XPath="@code">
						<p:restrictions>
							<p:defaultContent>
								<p:value>deg</p:value>
							</p:defaultContent>
						</p:restrictions>
						<p:input>
							<p:textField />
						</p:input>
					</p:elementString>
				</p:elements>
			</p:elementGroup>
			<p:elementDouble ID="value_latitude_Position" XPath="swe:value">
				<p:input>
					<p:textField />
				</p:input>
			</p:elementDouble>
		</p:elements>
	</p:elementGroup>

	<p:elementGroup groupID="altitude_Position" XPath="swe:field/swe:Quantity">
		<p:elements>
			<p:elementAnyURI ID="referenceFrame_altitude_Position" XPath="@referenceFrame">
				<p:restrictions>
					<p:fixContent>
						<p:value>urn:ogc:crs:EPSG:5715</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementAnyURI>
			<p:elementString ID="axisID_altitude_Position" XPath="@axisID">
				<p:restrictions>
					<p:fixContent>
						<p:value>Z</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementString>
			<p:elementGroup groupID="uom_altitude_Position" XPath="swe:uom">
				<p:elements>
					<p:elementString ID="code_uom_altitude_Position" XPath="@code">
						<p:restrictions>
							<p:defaultContent>
								<p:value>m</p:value>
							</p:defaultContent>
						</p:restrictions>
						<p:input>
							<p:textField />
						</p:input>
					</p:elementString>
				</p:elements>
			</p:elementGroup>
			<p:elementDouble ID="value_altitude_Position" XPath="swe:value">
				<p:input>
					<p:textField />
				</p:input>
			</p:elementDouble>
		</p:elements>
	</p:elementGroup>
	<p:formConfiguration>
		<p:formComponent label="Form">
			<p:element ref="description" />
			<p:elementGroup groupRef="KeywordList" />
			<p:elementGroup groupRef="Identification" />
			<p:elementGroup groupRef="TimePeriod" />
			<p:elementGroup groupRef="Contact" />
			<p:elementGroup groupRef="Position" />
		</p:formComponent>
	</p:formConfiguration>
</p:profile>
