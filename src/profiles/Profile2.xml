<?xml version="1.0" encoding="UTF-8"?>
<p:profile xmlns:p="http://xmlProfile.de" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlProfile.de Profile2.xsd">

	<p:element-group name="long name" XPath="sml:Term">
		<p:occurrence>
			<p:occur>1</p:occur>
		</p:occurrence>
		<p:elements>
			<p:element-group group-ref="short name" XPath="sml:Term/sml:start" />
			<p:element_string XPath="@identifier" datatype="string" id="2">
				<p:label></p:label>
				<p:description></p:description>
				<p:restrictions>
					<p:fix-content>
						<p:value>http://www.nexosproject.eu/dictionary/definitions.html#longName</p:value>
					</p:fix-content>
				</p:restrictions>
				<p:input>
					<p:textfield></p:textfield>
				</p:input>

			</p:element_string>
			<p:element_string XPath="sml:identifier/sml:label" datatype="string" id="identifier_label">
				<p:fix-content>
					<p:value>long name</p:value>
				</p:fix-content>
				
			</p:element_string>

			<p:element_string XPath="sml:identifier/@value" datatype="string" id="identifier_value" />
		</p:elements>
		<p:dependenceRules>
			<p:keys>
				<p:key name="Term_Identifier_key">
					<p:field>TermIdentifier</p:field>
				</p:key>
				<p:keyRef name="TermIdentifier_key" refer="Term_Identifier_key2">
					<p:field>TermIdentifier</p:field>
				</p:keyRef>
			</p:keys>
		</p:dependenceRules>
		<p:editorConfig>
			<p:label-group>Identification</p:label-group>
			<p:elements>
				<p:textField ref="identifier_value" datatype="string">
					<p:label>long name</p:label>
				</p:textField>
			</p:elements>
		</p:editorConfig>
	</p:element-group>


	<p:element-group name="short name" XPath="sml:Term/sml:start">
		<p:occurrence>
			<p:occur>1</p:occur>
		</p:occurrence>
		<p:elements>
			<p:element_string XPath="sml:Term/@identifier" datatype="string" id="TermIdentifier2">
				<p:fix-content>
					<p:value>http://www.nexosproject.eu/dictionary/definitions.html#longName</p:value>
				</p:fix-content>
			</p:element_string>
			<p:element_string XPath="sml:identifier/sml:label" datatype="string" id="identifier_label2">
				<p:fix-content>
					<p:value>long name</p:value>
					<!-- <p:ref refValueNotExist="fail">2</p:ref> -->
				</p:fix-content>
			</p:element_string>
			<p:element_anyType XPath="sml:identifier/@value" datatype="anyType" id="identifier_value2" />
		</p:elements>
		<p:dependenceRules>
			<p:logic>
				<p:AND>
					<p:element_anyTypeRef ref="identifier_value2" datatype="anyType">
						<p:fix-content>
							<p:ref datatype="anyType" ref="identifier_value2" refValueNotExist="fail" />
						</p:fix-content>
					</p:element_anyTypeRef>
					<p:element_anyTypeRef ref="identifier_value2" datatype="anyType">
						<p:fix-content>
							<p:ref datatype="anyType" ref="identifier_value2" refValueNotExist="fail" />
						</p:fix-content>
					</p:element_anyTypeRef>
				</p:AND>
			</p:logic>
			<p:keys>
				<p:key name="Term_Identifier_key2">
					<p:field>TermIdentifier</p:field>
				</p:key>
				<p:keyRef name="TermIdentifier_key2" refer="Term_Identifier_key2">
					<p:field>TermIdentifier</p:field>
				</p:keyRef>
			</p:keys>
		</p:dependenceRules>
		<p:editorConfig>
			<p:label-group>Identification</p:label-group>
			<p:elements>
				<p:textField ref="identifier_value2" datatype="anyType">
					<p:label>long name</p:label>
				</p:textField>
				<p:map longitude_ref="TermIdentifier2" datatype_longitude="string" datatype_latitude="string"
					latitude_ref="identifier_label2">
					<p:label>long name</p:label>
				</p:map>
			</p:elements>
		</p:editorConfig>
	</p:element-group>

	<p:element-group_dependenceRules>
		<p:implication>
			<p:if>
				<p:AND>
					<p:element-group group-ref="long name">
						<p:group-occurrence>
							<p:occur>2</p:occur>
						</p:group-occurrence>
					</p:element-group>
				</p:AND>
			</p:if>
			<p:then>
				<p:AND>
					<p:element-group group-ref="short name">
						<p:group-occurrence>
							<p:occur>2</p:occur>
						</p:group-occurrence>
					</p:element-group>
				</p:AND>
			</p:then>
		</p:implication>
	</p:element-group_dependenceRules>


	<p:editor_config_form>
		<p:form-component name="Identifier">
			<p:label>Identifier</p:label>
			<p:help>This component groups the identifiers</p:help>
			<p:element-group_choice label="Identifiers">
				<p:element-group name="short name" />
				<p:element-group name="long name" />
			</p:element-group_choice>
		</p:form-component>
	</p:editor_config_form>
</p:profile>
