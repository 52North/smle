{
  "name": "smle",
  "version": "1.0.0",
  "description": "The Friendly SensorML Editor",
  "author": "Christian Autermann <c.autermann@52north.org>",
  "homepage": "https://github.com/auterman/smle",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/autermann/smle.git"
  },
  "bugs": {
    "url": "https://github.com/autermann/smle/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "ci": "npm run lint && npm test && npm run e2e",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run protractor",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "postinstall": "npm run webdriver:update",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "pree2e": "npm run webdriver:update -- --standalone",
    "preversion": "npm test",
    "protractor": "protractor",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist --cors",
    "server": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run server:dev",
    "test": "karma start",
    "tslint": "tslint 'src/app/**/*.ts'",
    "typedoc": "typedoc",
    "version": "npm run build",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "@angular/common": "^2.4.9",
    "@angular/compiler": "^2.4.9",
    "@angular/core": "^2.4.9",
    "@angular/forms": "^2.4.9",
    "@angular/http": "^2.4.9",
    "@angular/platform-browser": "^2.4.9",
    "@angular/platform-browser-dynamic": "^2.4.9",
    "@angular/platform-server": "^2.4.9",
    "@angular/router": "^3.4.9",
    "@angularclass/hmr-loader": "~3.0.2",
    "angular2-modal": "2.0.3",
    "angular2-tree-component": "2.8.1",
    "angular2-uuid": "^1.0.7",
    "bootstrap": "4.0.0-alpha.2",
    "core-js": "^2.4.1",
    "font-awesome": "^4.5.0",
    "jquery": "^2.2.4",
    "jquery-ui": "^1.10.5",
    "jquery-ui-timepicker-addon": "^1.5.5",
    "leaflet": "^1.0.3",
    "moment": "^2.17.1",
    "primeng": "1.0.0-beta.16",
    "primeui": "^4.1.15",
    "rxjs": "5.2.0",
    "tether": "^1.3.3",
    "typescript": "2.2.1",
    "typescript-logging": "^0.2.1",
    "x2js": "^3.0.1",
    "zone.js": "0.7.8"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.5.44",
    "@types/node": "^7.0.8",
    "@types/selenium-webdriver": "^2.53.42",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.0.27",
    "@types/webpack": "^2.2.11",
    "angular2-template-loader": "^0.6.2",
    "assets-webpack-plugin": "^3.4.0",
    "awesome-typescript-loader": "3.1.2",
    "codelyzer": "^2.0.1",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.27.1",
    "es6-promise": "^3.1.2",
    "es6-promise-loader": "^1.0.1",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "exports-loader": "^0.6.4",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.21.0",
    "http-server": "^0.9.0",
    "ie-shim": "^0.1.0",
    "imports-loader": "^0.7.1",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.3",
    "karma-remap-coverage": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "node-sass": "^4.5.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.8.1",
    "protractor": "^3.2.2",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.6.3",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^5.0.1",
    "source-map-loader": "^0.1.5",
    "string-replace-loader": "^1.0.5",
    "style-loader": "^0.13.2",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "^1.1.1",
    "ts-node": "^2.0.0",
    "tslib": "^1.6.0",
    "tslint": "4.5.1",
    "tslint-loader": "^3.4.3",
    "typedoc": "^0.5.7",
    "url-loader": "^0.5.8",
    "w3c-schemas": "^1.4.0",
    "webpack": "2.2.1",
    "webpack-deploy2war": "^0.1.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "2.4.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "2.6.1"
  }
}
